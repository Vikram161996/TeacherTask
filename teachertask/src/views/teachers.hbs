<div class="container">
  <h1>Teacher Details</h1>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">Action</th>
      </tr>
    </thead>
    <tbody>
      {{#each data}}
      <tr>
        <th scope="row">{{this.id}}</th>
        <td><a href="/students/{{this.id}}">{{this.firstName}}</a> </td>
        <td>{{this.lastName}}</td>
        <td><a href="/edit-teacher/{{this.id}}">Edit</a></td>
      </tr>
      {{/each}}
    </tbody>
  </table>
  <a class="btn btn-primary btn-lg" href="/add-teacher">Add Teacher</a>
</div>